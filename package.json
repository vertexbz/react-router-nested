{
  "name": "react-router-nested",
  "version": "4.2.2",
  "description": "React Router addon for nesting in for declaring nested paths without knowledge of what was the parent url for current scope.",
  "main": "lib/index.js",
  "author": "Adam Mak≈õwiej <adam.makswiej@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/vertexbz/react-router-nested.git"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "router",
    "react-router",
    "nested",
    "stack",
    "layer",
    "scope",
    "relative",
    "navigation",
    "hoc",
    "link"
  ],
  "dependencies": {
    "classnames": "^2.2.5",
    "history": "^4.7.2",
    "hoist-non-react-statics": "^2.3.1",
    "invariant": "^2.2.2",
    "object.omit": "^3.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-display-name": "^0.2.3",
    "react-router": "^4.2.0",
    "recursive-proxy": "^1.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.14.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^2.40.1",
    "eslint-plugin-no-only-tests": "^2.0.0",
    "eslint-plugin-react": "^7.5.1",
    "flow": "^0.2.3",
    "flow-bin": "^0.64.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.0.6",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "build:flow": "flow gen-flow-files src/index.js --out-dir lib/",
    "build:src": "babel src --out-dir lib -s",
    "build": "yarn lint && yarn flow && rm -rf ./lib/ && yarn build:src",
    "lint": "eslint src --ext .js --ext .jsx --cache",
    "lint:fix": "eslint src --ext .js --ext .jsx --cache --fix",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>config/test.js",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/config/"
    ]
  }
}
